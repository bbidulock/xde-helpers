dist_noinst_DATA = \
	$(hicolor_ICONS) \
	$(pixmap_ICONS)

install-data-hook:
	$(NORMAL_INSTALL)
	@x="$(DESTDIR)$(icondir)/" ; \
	test -d "$$x" || $(MKDIR_P) "$$x" ; \
	list="$(hicolor_ICONS)" ; for f in $$list ; do \
		p=`basename "$$f" .svg`.png ; \
		g=`basename "$$f" .svg`.svg ; \
		d=; if test -f "$(srcdir)/$$f" ; then d="$(srcdir)/" ; fi ; \
		t="$(DESTDIR)$(datadir)/icons/hicolor/scalable/apps/" ; \
		test -d "$$t" || $(MKDIR_P) "$$t" ; \
		echo "  INST  $$t$$g" ; \
		rsvg-convert -w 48 -h 48 -z 1.0 -f svg "$$d$$f" | sed -e 's,"48pt","48px",g' >"$$t$$g" ; \
		echo "  INST  $$x$$g" ; \
		rm -f "$$x$$g" ; ln -sf "../../icons/hicolor/scalable/apps/$$g" "$$x$$g" ; \
		if test "$$p" = "xlogo.png" -o "$$p" = "arch-logo.png"; then \
			sizes="16 22 24 32 36 48 56 64 72 96 100 128 256" ; \
		else \
			sizes="16 24 32 48 56 64" ; \
		fi ; \
		for s in $$sizes ; do \
			t="$(DESTDIR)$(datadir)/icons/hicolor/$${s}x$${s}/apps/" ; \
			test -d "$$t" || $(MKDIR_P) "$$t" ; \
			echo "  INST  $$t$$p" ; \
			rsvg-convert -w $$s -h $$s -z 1.0 -f png "$$x$$g" -o "$$t$$p" ; \
		done ; \
		echo "  INST  $$x$$p" ; \
		rm -f "$$x$$p" ; ln -sf "../../icons/hicolor/48x48/apps/$$p" "$$x$$p" ; \
	done ; \
	list="$(pixmap_ICONS)" ; for f in $$list ; do \
		d=; if test -f "$(srcdir)/$$f" ; then d="$(srcdir)/" ; fi ; \
		case "$$f" in \
		*.png) \
			echo " INST  $$x$$f" ; \
			$(INSTALL_DATA) "$$d$$f" "$$x" ;; \
		*.xpm) \
			echo " INST  $$x$$f" ; \
			$(INSTALL_DATA) "$$d$$f" "$$x" ;; \
		*.svg) \
			p=`basename "$$f" .svg`.png ; \
			g=`basename "$$f" .svg`.svg ; \
			echo "  INST  $$x$$g" ; \
			rsvg-convert -w 48 -h 48 -z 1.0 -f svg "$$d$$f" | sed -e 's,"48pt","48px",g' >"$$x$$g" ; \
			echo "  INST  $$x$$p" ; \
			rsvg-convert -w 48 -h 48 -z 1.0 -f png "$$x$$g" -o "$$x$$p" ;; \
		esac ; \
	done

uninstall-hook:
	$(NORMAL_UNINSTALL)
	@x="$(DESTDIR)$(icondir)/" ; \
	list="$(pixmap_ICONS)" ; for f in $$list ; do \
		case "$$f" in \
		*.png) \
			p=`basename "$$f"` ; rm -fv "$$x$$p" ;; \
		*.xpm) \
			p=`basename "$$f"` ; rm -fv "$$x$$p" ;; \
		*.svg) \
			p=`basename "$$f" .svg`.png ; rm -fv "$$x$$p" ; \
			g=`basename "$$f" .svg`.svg ; rm -fv "$$x$$g" ;; \
		esac ; \
	done

