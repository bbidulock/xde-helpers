[Window Manager]
SessionManaged=true

[Desktop Entry]
Name=AfterStep
Terminal=False
Type=Application
Comment=AfterStep is feature rich NeXTish window manager.
# Stolen from metacity : 
NoDisplay=true
# name of loadable control center module
# X-GNOME-WMSettingsModule=
# name we put on the WM spec check window
X-GNOME-WMName=AfterStep
# back compat only 
#X-GnomeWMSettingsLibrary=
#X-GNOME-Bugzilla-Bugzilla=
#X-GNOME-Bugzilla-Product=
#X-GNOME-Bugzilla-Component=
X-GNOME-Autostart-Phase=WindowManager
X-GNOME-Provides=windowmanager
X-GNOME-Autostart-Notify=true

Exec=afterstep
Icon=AfterStep
Actions=XDE;

[Desktop Action XDE]
Name=XDE
Icon=xde
Comment=AfterStep in an XDE session
TryExec=xde-session
Exec=xde-session -desktop AFTERSTEP -startwm afterstep
