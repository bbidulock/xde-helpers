[Desktop Entry]
Type=Application
Encoding=UTF-8
Name=wmx
Exec=wmx
NoDisplay=true
X-GNOME-WMName=wmx
X-GNOME-Autostart-Phase=WindowManager
X-GNOME-Provides=windowmanager
X-GNOME-Autostart-Notify=true
Actions=XDE;

[Desktop Action XDE]
Name=XDE
Icon=xde
TryExec=xde-session
Exec=xde-session -desktop WMX -startwm start-wmx
