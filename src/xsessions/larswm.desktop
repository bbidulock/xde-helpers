[Desktop Entry]
Encoding=UTF-8
Name=larswm
Comment=A tiling window manager based on 9wm
Exec=larswm
Icon=larswm
TryExec=larswm
Type=XSession
Actions=XDE;

[Desktop Action XDE]
Name=XDE
Icon=xde
Comment=larswm in an XDE session
TryExec=xde-session
Exec=xde-session -desktop LARSWM -startwm start-larswm
