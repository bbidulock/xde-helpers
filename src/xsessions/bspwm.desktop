[Desktop Entry]
Encoding=UTF-8
Name=bspwm
Comment=A tiling window manager base on binary space partitioning
Exec=bspwm
Icon=bspwm
Type=XSession
Actions=XDE;

[Desktop Action XDE]
Name=XDE
Icon=xde
Comment=bswpm in an XDE session
TryExec=xde-session
Exec=xde-session -desktop BSPWM -startwm start-bspwm
