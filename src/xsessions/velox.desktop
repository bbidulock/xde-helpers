# velox.desktop

[Desktop Entry] 
Encoding=UTF-8
Name=velox
Icon=velox
Comment=Simple tiling window manager
TryExec=velox
Exec=velox
Actions=XDE;

[Desktop Action XDE]
Name=XDE
Icon=xde
Comment=velox under an XDE session
TryExec=xde-session
Exec=xde-session -exec uxterm -desktop VELOX -startwm velox

