[Desktop Entry]
Encoding=UTF-8
Type=XSession
Name=wm2
GenericName=Window Manager
Comment=The ideal window manager for today's ascetic programmer
Exec=wm2
Icon=wm2
Actions=XDE;

[Window Manager]
SessionManaged=true
