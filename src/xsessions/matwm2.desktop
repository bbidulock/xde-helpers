[Desktop Entry]
Encoding=UTF-8
Name=matwm2
Comment=A nice tiling or floating window manager
Exec=matwm2
Icon=matwm2
Type=XSession
NoDisplay=true
Actions=XDE;

[Desktop Action XDE]
Name=XDE
Icon=xde
Comment=matwm2 under an XDE session
TryExec=xde-session
Exec=xde-session -desktop MATWM2 -startwm start-matwm2
