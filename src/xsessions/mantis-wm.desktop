[Desktop Entry]
Encoding=UTF-8
Name=mantis-wm
Comment=Lightweight window manager for X
Exec=mantis-wm
Icon=mantis-wm
Type=XSession
NoDisplay=true
Actions=XDE;

[Desktop Action XDE]
Name=XDE
Icon=xde
Comment=Mantis under an XDE session
TryExec=xde-session
Exec=xde-session -desktop MANTIS -startwm start-mantis
