[Desktop Entry]
Encoding=UTF-8
Type=Application
Name=XDE Panel
GenericName=Panel
Comment=Panel for X Desktop Environment.
Icon=perlpanel-panel
Exec=sh -c "xde-panel $HOME/.config/xde-panel/${XDG_CURRENT_DESKTOP:-default}/rc.xml"
TryExec=xde-panel
Terminal=false
Categories=XDE;System;Utility;
OnlyShowIn=FLUXBOX;BLACKBOX;OPENBOX;PEKWM;WMAKER;
StartupNotify=true
StartupWMClass=xde-panel
# only launch for WM's with lacking or no panel
