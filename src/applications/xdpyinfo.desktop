[Desktop Entry]
Type=Application
Name=X Display Properties
GenericName=xdpyinfo
Comment=List properties of the X display
Icon=xlogo
TryExec=/usr/bin/gxmessage
Exec=/usr/bin/xdpyinfo | gxmessage -name xdpyinfo -geometry 450x450 -font 'Liberation Mono 8' -title 'X Display Properties' -file -
Categories=Application;X-X11;X-XTool;
StartupNotify=false
StartupWMClass=xdpyinfo
X-AppInstall-Package=xorg-xdpyinfo
X-AppInstall-Section=extra
X-Arch-Package=xorg-xdpyinfo 1.3.2-1
X-Arch-Package-Repository=extra
X-Arch-Package-Name=xorg-xdpyinfo
X-Arch-Package-Version=1.3.2-1
X-Arch-Package-Description=Display information utility for X
X-Arch-Package-Groups=xorg-apps  xorg
X-Arch-Package-URL=http://xorg.freedesktop.org/
