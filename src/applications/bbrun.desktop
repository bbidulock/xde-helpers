[Desktop Entry]
Encoding=UTF-8
Type=Application
Name=Run Command
GenericName=Launcher
Comment=Run a command from a DockApp
Icon=system-run
OnlyShowIn=BLACKBOX;
TryExec=/usr/bin/bbrun
Exec=bbrun -a -w
Terminal=false
Categories=System;Utility;DockApp;WMAKER;
StartupNotify=false
StartupWMClass=bbrun
X-GNOME-Autostart-enabled=true
#Broken when you hit ENTER instead of OK
Hidden=true
