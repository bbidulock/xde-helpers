[Desktop Entry]
Encoding=UTF-8
Type=Application
Name=X RandR
GenericName=XRandr
Comment=List the resolution properties of the monitor
Icon=xlogo
TryExec=/usr/bin/xrandr
Exec=xrandr | gxmessage -font 'monospace 10' -title 'X Window Properties' -file -
Terminal=false
Categories=X-X11;Application;X-XTool;
StartupNotify=false
