[Desktop Entry]
Encoding=UTF-8
Type=Application
Name=X Window Information
GenericName=XWinInfo
Comment=List the children of the root window
Icon=xlogo
TryExec=/usr/bin/xwininfo
Exec=xwininfo -root -children | gxmessage -font 'monospace 10' -title 'X Windows' -file -
Terminal=false
Categories=X-X11;Application;X-XTool;
StartupNotify=false
