[Desktop Entry]
Type=Application
Name=xplanetFX
GenericName=xplanetFX
Comment=xplanetFX - renderings of planet earth as desktop wallpaper
Icon=/usr/share/pixmaps/xplanetFX.svg
TryExec=/usr/bin/xplanetFX
Exec=xplanetFX --gui
Categories=Application;Utility;
StartupNotify=false
X-AppInstall-Package=xplanetfx
X-AppInstall-Section=custom
X-Arch-Package=xplanetfx 2.6.5-1
X-Arch-Package-Repository=custom
X-Arch-Package-Name=xplanetfx
X-Arch-Package-Version=2.6.5-1
X-Arch-Package-Description=xplanet earth with realistic clouding and lightning effects as your wallpaper.
X-Arch-Package-Groups=None
X-Arch-Package-URL=http://mein-neues-blog.de/xplanetfx/
