[Desktop Entry]
Version=1.0
Encoding=UTF-8
Type=Application
Name=X11VNC Server
Comment=Share this desktop by VNC
Icon=computer
Exec=x11vnc -gui tray=setpass -rfbport PROMPT -bg -o %%HOME/.x11vnc.log.%%VNCDISPLAY
Terminal=false
Categories=Network;RemoteAccess;
StartupNotify=false
X-AppInstall-Package=x11vnc
X-AppInstall-Popcon=775
X-AppInstall-Section=main
