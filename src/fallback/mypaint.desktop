[Desktop Entry]
Version=1.0
Encoding=UTF-8
Type=Application
Name=MyPaint
GenericName=Image Editor
Comment=Paint images from scratch
Icon=mypaint
TryExec=/usr/bin/mypaint
Exec=mypaint %f
Terminal=false
Categories=Graphics;
MimeType=image/openraster;image/png;image/jpeg;
StartupNotify=false
X-AppInstall-Package=mypaint
X-AppInstall-Popcon=47
X-AppInstall-Section=main
X-Arch-Package-Name=mypaint
X-Arch-Package-Version=1.1.0-3
X-Arch-Package-Description=A fast and easy painting application for digital painters, with brush dynamics
X-Arch-Package-URL=http://mypaint.intilinux.com/
